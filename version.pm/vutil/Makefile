# --- MakeMaker ppd section:
# Creates a PPD (Perl Package Description) for a binary distribution.
ppd :
	$(NOECHO) $(ECHO) "<SOFTPKG NAME=\"version-vxs\" VERSION=\"0.9928\">" > version-vxs.ppd
	$(NOECHO) $(ECHO) "    <ABSTRACT></ABSTRACT>" >> version-vxs.ppd
	$(NOECHO) $(ECHO) "    <AUTHOR>John Peacock &lt;jpeacock@cpan.org&gt;</AUTHOR>" >> version-vxs.ppd
	$(NOECHO) $(ECHO) "    <IMPLEMENTATION>" >> version-vxs.ppd
	$(NOECHO) $(ECHO) "        <ARCHITECTURE NAME=\"MSWin32-x64-multi-thread-5.28\" />" >> version-vxs.ppd
	$(NOECHO) $(ECHO) "        <CODEBASE HREF=\"\" />" >> version-vxs.ppd
	$(NOECHO) $(ECHO) "    </IMPLEMENTATION>" >> version-vxs.ppd
	$(NOECHO) $(ECHO) ^</SOFTPKG^> >> version-vxs.ppd


